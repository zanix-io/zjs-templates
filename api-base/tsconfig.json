{
  "extends": "@zanix/std-config/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "services/*": ["src/app/services/*"],
      "adapters/*": ["src/app/services/adapters/*"],
      "providers/*": ["src/commons/(providers)/*"],
      "clients/*": ["src/commons/(clients)/*"],
      "models/*": ["src/commons/models/*"],
      "middlewares": ["src/commons/middlewares/index"],
      "utilities": ["src/commons/utils/index"]
    }
  }
}
